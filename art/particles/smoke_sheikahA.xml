<?xml version="1.0" encoding="utf-8"?>
<particles>

    <texture>art/textures/particles/dust_256a.png</texture>
    <blend mode="over"/>
	
	<start_full/>
	
    <constant name="emissionrate" value="25.0"/>
    <uniform name="lifetime" min="3.0" max="15.0"/>

    <uniform name="position.x" min="-1.0" max="1.0"/>
    <uniform name="position.z" min="-1.0" max="1.0"/>
    <constant name="position.y" value="0.0"/>

    <uniform name="angle" min="-3.14" max="3.14"/>

    <uniform name="velocity.x" min="-0.75" max="0.75"/>
    <uniform name="velocity.y" min="0.1" max="0.75"/>
    <uniform name="velocity.z" min="-0.75" max="0.75"/>
    <uniform name="velocity.angle" min="-1.5" max="1.5"/>

    <uniform name="size" min="2.0" max="5.5"/>

    <force y="-0.1"/>

</particles>
